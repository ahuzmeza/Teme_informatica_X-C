#include <iostream>
using namespace std;

    int n, v[20];

int valid(int k);
int solutie(int k);
void afisare(int k);
void Backtrack(int k);

int main()
{
    int cnt = 0;
        cin>>n;

    Backtrack(1);

return 0;
}

void Backtrack(int k)
{
    int i;
    for (i = 1; i <= n; i++)
    {
        v[k]=i;
           if (valid(k))
            {
                if (solutie(k))
                    afisare(k);
                else
                    Backtrack(k+1);
            }
    }
}


int valid(int k)
{
    for (int i=1;i<=k-1;i++)
        if (v[i] == v[k])
                return 0;
    return 1;
}

int solutie(int k)
{
    if (k == n)
        return 1;
return 0;
}

void afisare(int k)
{
    for (int i = 1; i <= k; i++)
        cout<< v[i] <<" ";
    cout<< endl;
}








