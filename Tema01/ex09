#include <iostream>
using namespace std;

    int n, v[20];
    char cuv[5];

int valid(int k);
int solutie(int k);
void afisare(int k);
void Backtrack(int k);

int main()
{
    n = 4;
    cout<< endl;
    Backtrack(1);

return 0;
}

void Backtrack(int k)
{
    int i;
    for (i = 0; i < n; i++)
    {
        cuv[k]= 'a' + i;
            if (valid(k))
            {
                if (solutie(k))
                    afisare(k);
                else
                    Backtrack(k+1);
            }
    }
}


int valid(int k)
{
    if (k > 1)
        if (cuv[k-1] == cuv[k])
            return 0;
    return 1;
}

int solutie(int k)
{
    if (k == n)
        return 1;
return 0;
}

void afisare(int k)
{
    for (int i = 1; i <= k; i++)
        cout<< cuv[i] <<" ";
    cout<< endl;
}






